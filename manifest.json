{
  "manifest_version": 3,
  "name": "SessionVault",
  "version": "1.0.0",
  "description": "Capture and manage your browser sessions",
  "permissions": [
    "storage",
    "tabs",
    "windows",
    "bookmarks",
    "identity"
  ],
  "host_permissions": [
    "https://api.notion.com/*"
  ],
  "background": {
    "service_worker": "background/service-worker.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "public/icons/icon16.png",
      "48": "public/icons/icon48.png",
      "128": "public/icons/icon128.png"
    }
  },
  "chrome_url_overrides": {},
  "icons": {
    "16": "public/icons/icon16.png",
    "48": "public/icons/icon48.png",
    "128": "public/icons/icon128.png"
  }
}

